{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb",
       "airbnb-typescript"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "project": "./tsconfig.json"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/prefer-default-export": "off",
        "react/function-component-definition": "off",
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "import/no-extraneous-dependencies": ["error", {"devDependencies": true}]


    }
}